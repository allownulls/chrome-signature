{
	"manifest_version": 2,
	"name": "Custom Google Homepage",
	"description": "This extension changes the background image of the google homepage",
	"version": "0.99",
	"options_page": "options.html",
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	},
	"content_scripts": [
 {
	"matches": ["<all_urls>"],
	"all_frames": true,
	"css": ["main.css"],
	"run_at": "document_end"
 }
 ],
	"background": {
	"scripts": [
		"keycodes.js",
		"tabs.js",
		"background.js",
		"jsrsasign-all-min.js"
	]
	},

	"browser_action": {
	"default_icon": "cvproof-finger.png",
	"default_title": "CVProof fingerprint"
	},
	"permissions": [
		"<all_urls>",
		"tts",
		"tabs",
		"storage"
	]
}